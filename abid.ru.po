msgctxt "tformdetails.labelsalomtr.caption"
msgid ""
"Ma'nosi:\n"
"Sizga tinchlik-omonlik, Аllohning rahmati boʼlsin.\n"
msgstr ""
"Значение:\n"
"Мир вам и милость Аллаха.\n"

# Do not translate
#: tformdetails.labelsanoar.caption
